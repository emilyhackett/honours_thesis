
@thesis{chin_parametric_2016,
	title = {Parametric Imaging of Tissue Microstructure using Optical Coherence Tomography : Birefringence and Elasticity},
	url = {http://research-repository.uwa.edu.au/en/publications/record(66bc309f-b19b-43d4-83ad-a4e047b84d09).html'},
	abstract = {Biological tissue has intrinsic optical and mechanical properties that are linked to its structure and function, and are altered by pathologies such as cancer. Techniques for imaging these properties could, thus, aid in the detection, diagnosis, and understanding of disease. Birefringence is one such optical property, arising from structural and molecular anisotropies in tissue and, thus, a measure of the tissue microstructure. Similarly, many pathologies lead to a stiffening or softening of tissues, and, in principle, can be detected from changes in mechanical elasticity. Optical coherence tomography ({OCT}) is a minimally invasive optical imaging modality, with micrometre-scale resolution and millimetre-scale depth penetration, which can be extended to measure these tissue properties. This research combines three-dimensional {OCT} imaging with parametric modelling to develop signal processing methods for spatially resolved micro-scale measurements of tissue birefringence and elasticity, an approach referred to as parametric {OCT}.},
	institution = {University of Western Australia},
	type = {phdthesis},
	author = {Chin, L.},
	date = {2016},
	keywords = {{BRITElab}, Elastography, {OCT}},
	file = {Chin2016PhdUWASEEE_Parametric imaging of tissue microstructure using OCT....pdf:/Users/emilyhackett/Zotero/storage/UD4Z82XW/Chin2016PhdUWASEEE_Parametric imaging of tissue microstructure using OCT....pdf:application/pdf}
}

@article{kennedy_strain_2012,
	title = {Strain Estimation in Phase-Sensitive Optical Coherence Elastography},
	volume = {3},
	url = {http://www.osapublishing.org/boe/abstract.cfm?URI=boe-3-8-1865},
	doi = {10.1364/BOE.3.001865},
	abstract = {We present a theoretical framework for strain estimation in optical coherence elastography ({OCE}), based on a statistical analysis of displacement measurements obtained from a mechanically loaded sample. We define strain sensitivity, signal-to-noise ratio and dynamic range, and derive estimates of strain using three methods: finite difference, ordinary least squares and weighted least squares, the latter implemented for the first time in {OCE}. We compare theoretical predictions with experimental results and demonstrate a 12 {dB} improvement in strain sensitivity using weighted least squares compared to finite difference strain estimation and a 4 {dB} improvement over ordinary least squares strain estimation. We present strain images (i.e., elastograms) of tissue-mimicking phantoms and excised porcine airway, demonstrating in each case clear contrast based on the sample's elasticity.},
	pages = {1865--1879},
	number = {8},
	journaltitle = {Biomed. Opt. Express},
	author = {Kennedy, B.F. and Koh, S.H. and {McLaughlin}, R.A. and Kennedy, K.M. and Munro, P.R.T. and Sampson, D.D.},
	date = {2012-08},
	keywords = {Optical coherence tomography, Scattering measurements, Tissue characterization},
	file = {Kennedy2012BOEv3n8p1865_Strain estimation in phase-sensitive OCE.pdf:/Users/emilyhackett/Zotero/storage/KMUI2HLB/Kennedy2012BOEv3n8p1865_Strain estimation in phase-sensitive OCE.pdf:application/pdf}
}

@article{kennedy_optical_2014,
	title = {Optical Coherence Micro-Elastography: Mechanical-contrast Imaging of Tissue Microstructure},
	volume = {5},
	url = {http://www.osapublishing.org/boe/abstract.cfm?URI=boe-5-7-2113},
	doi = {10.1364/BOE.5.002113},
	abstract = {We present optical coherence micro-elastography, an improved form of compression optical coherence elastography. We demonstrate the capacity of this technique to produce en face images, closely corresponding with histology, that reveal micro-scale mechanical contrast in human breast and lymph node tissues. We use phase-sensitive, three-dimensional optical coherence tomography ({OCT}) to probe the nanometer-to-micrometer-scale axial displacements in tissues induced by compressive loading. Optical coherence micro-elastography incorporates common-path interferometry, weighted averaging of the complex {OCT} signal and weighted least-squares regression. Using three-dimensional phase unwrapping, we have increased the maximum detectable strain eleven-fold over no unwrapping and the minimum detectable strain is 2.6 μϵ. We demonstrate the potential of mechanical over optical contrast for visualizing micro-scale tissue structures in human breast cancer pathology and lymph node morphology.},
	pages = {2113--2124},
	number = {7},
	journaltitle = {Biomed. Opt. Express},
	author = {Kennedy, B.F. and {McLaughlin}, R.A. and Kennedy, K.M. and Chin, L. and Curatolo, A. and Tien, A. and Latham, B. and Saunders, C.M. and Sampson, D.D.},
	date = {2014-07},
	keywords = {Optical coherence tomography, Coherence imaging, Phase unwrapping},
	file = {Kennedy2014BOEv5n7p2113_Optical coherence micro-elastography....pdf:/Users/emilyhackett/Zotero/storage/9MHLU2K4/Kennedy2014BOEv5n7p2113_Optical coherence micro-elastography....pdf:application/pdf}
}

