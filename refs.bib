	url = {http://www.osapublishing.org/boe/abstract.cfm?URI=boe-7-10-4139},
	doi = {10.1364/BOE.7.004139},
	abstract = {Incomplete excision of malignant tissue is a major issue in breast-conserving surgery, with typically 20 - 30\% of cases requiring a second surgical procedure arising from postoperative detection of an involved margin. We report advances in the development of a new intraoperative tool, optical coherence micro-elastography, for the assessment of tumor margins on the micro-scale. We demonstrate an important step by conducting whole specimen imaging in intraoperative time frames with a wide-field scanning system acquiring mosaicked elastograms with overall dimensions of 50 \&\#x00D7; 50 mm, large enough to image an entire face of most lumpectomy specimens. This capability is enabled by a wide-aperture annular actuator with an internal diameter of 65 mm. We demonstrate feasibility by presenting elastograms recorded from freshly excised human breast tissue, including from a mastectomy, lumpectomies and a cavity shaving.},
	pages = {4139--4153},
	number = {10},
	journaltitle = {Biomed. Opt. Express},
	author = {Allen, W.M. and Chin, L. and Wijesinghe, P. and Kirk, R.W. and Latham, B. and Sampson, D.D. and Saunders, C.M. and Kennedy, B.F.},
	date = {2016-10},
	keywords = {Optical coherence tomography, Medical optics and biotechnology},
	file = {boe-7-10-4139.pdf:/Users/emilyhackett/Zotero/storage/KX8S83Z7/boe-7-10-4139.pdf:application/pdf}
}

